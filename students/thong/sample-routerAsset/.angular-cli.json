{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "sample-router-asset"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "./assets/auto-hint": [
        "./assets/auto-hint",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "./assets/auto-hint/fonts/icomoon/icomoon.css",
        "./assets/auto-hint/js/plugins/fullcalendar/fullcalendar.min.css",
        "./assets/auto-hint/js/plugins/magnific/magnific-popup.css",
        "./assets/auto-hint/js/plugins/c3charts/c3.min.css",
        "./assets/auto-hint/skin/default_skin/css/theme.css",
        "./assets/auto-hint/allcp/forms/css/forms.css"
       
      ],
      "scripts": [
        "./assets/auto-hint/js/jquery/jquery-1.11.3.min.js",
        "./assets/auto-hint/js/jquery/jquery_ui/jquery-ui.min.js",
        "./assets/auto-hint/js/plugins/c3charts/d3.min.js",
        "./assets/auto-hint/js/plugins/c3charts/c3.min.js",
        "./assets/auto-hint/js/plugins/circles/circles.js",
        "./assets/auto-hint/js/plugins/jvectormap/jquery.jvectormap.min.js",
        "./assets/auto-hint/js/plugins/jvectormap/jquery.jvectormap.min.js",
        "./assets/auto-hint/js/plugins/jvectormap/assets/jquery-jvectormap-us-lcc-en.js",
        "./assets/auto-hint/js/plugins/fullcalendar/lib/moment.min.js",
        "./assets/auto-hint/js/plugins/fullcalendar/fullcalendar.min.js",
        "./assets/auto-hint/allcp/forms/js/jquery-ui-monthpicker.min.js",
        "./assets/auto-hint/allcp/forms/js/jquery-ui-datepicker.min.js",
        "./assets/auto-hint/js/plugins/magnific/jquery.magnific-popup.js",
        "./assets/auto-hint/js/utility/utility.js",

        "./assets/auto-hint/js/demo/demo.js",
        "./assets/auto-hint/js/main.js",
        "./assets/auto-hint/js/demo/widgets.js",
        "./assets/auto-hint/js/demo/widgets_sidebar.js",
        "./assets/auto-hint/js/pages/dashboard1.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}