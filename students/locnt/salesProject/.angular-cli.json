        "assets/AutoHintTheme/new/css/ace-rtl.min.css"
        
      ],
      "scripts": [
        "assets/AutoHintTheme/js/jquery/jquery-1.11.3.min.js",
        "assets/AutoHintTheme/js/jquery/jquery_ui/jquery-ui.min.js",
        "assets/AutoHintTheme/js/plugins/highcharts/highcharts.js",
        "assets/AutoHintTheme/js/plugins/c3charts/d3.min.js",
        "assets/AutoHintTheme/js/plugins/c3charts/c3.min.js",
        "assets/AutoHintTheme/js/plugins/circles/circles.js",
        "assets/AutoHintTheme/js/plugins/jvectormap/jquery.jvectormap.min.js",
        "assets/AutoHintTheme/js/plugins/jvectormap/assets/jquery-jvectormap-us-lcc-en.js",
        "assets/AutoHintTheme/js/plugins/fullcalendar/lib/moment.min.js",
        "assets/AutoHintTheme/js/plugins/fullcalendar/fullcalendar.min.js",
        "assets/AutoHintTheme/allcp/forms/js/jquery-ui-monthpicker.min.js",
        "assets/AutoHintTheme/allcp/forms/js/jquery-ui-datepicker.min.js",
        "assets/AutoHintTheme/js/plugins/magnific/jquery.magnific-popup.js",
        "assets/AutoHintTheme/js/utility/utility.js",
        "assets/AutoHintTheme/js/demo/demo.js",
        "assets/AutoHintTheme/js/main.js",
        "assets/AutoHintTheme/js/demo/widgets.js",
        "assets/AutoHintTheme/js/demo/widgets_sidebar.js",
        "assets/AutoHintTheme/js/pages/dashboard1.js",

        "assets/AutoHintTheme/new/js/ace-extra.min.js",
        "assets/AutoHintTheme/new/js/jquery-ui.custom.min.js",
        "assets/AutoHintTheme/new/js/jquery.ui.touch-punch.min.js",
        "assets/AutoHintTheme/new/js/jquery.gritter.min.js",
        "assets/AutoHintTheme/new/js/bootbox.js",
        "assets/AutoHintTheme/new/js/jquery.easypiechart.min.js",
        "assets/AutoHintTheme/new/js/bootstrap-datepicker.min.js",
        "assets/AutoHintTheme/new/js/jquery.hotkeys.index.min.js",
        "assets/AutoHintTheme/new/js/bootstrap-wysiwyg.min.js",
        "assets/AutoHintTheme/new/js/select2.min.js",
        "assets/AutoHintTheme/new/js/spinbox.min.js",
        "assets/AutoHintTheme/new/js/bootstrap-editable.min.js",
        "assets/AutoHintTheme/new/js/ace-editable.min.js",
        "assets/AutoHintTheme/new/js/jquery.maskedinput.min.js"
