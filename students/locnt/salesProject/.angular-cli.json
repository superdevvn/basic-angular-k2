{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "sales-project"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/AutoHintTheme/fonts/icomoon/icomoon.css",
        "assets/AutoHintTheme/js/plugins/fullcalendar/fullcalendar.min.css",
        "assets/AutoHintTheme/js/plugins/magnific/magnific-popup.css",
        "assets/AutoHintTheme/js/plugins/c3charts/c3.min.css",
        "assets/AutoHintTheme/skin/default_skin/css/theme.css",
        "assets/AutoHintTheme/allcp/forms/css/forms.css",

        "assets/AutoHintTheme/new/css/bootstrap-editable.min.css",
        "assets/AutoHintTheme/new/css/bootstrap.min.css",
        "assets/AutoHintTheme/new/font-awesome/4.5.0/css/font-awesome.min.css",
        "assets/AutoHintTheme/new/css/jquery-ui.custom.min.css",
        "assets/AutoHintTheme/new/css/jquery.gritter.min.css",
        "assets/AutoHintTheme/new/css/select2.min.css",
        "assets/AutoHintTheme/new/css/bootstrap-datepicker3.min.css",
